{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": [
        "airbnb-typescript",
        "prettier",
        "plugin:import/recommended",
        "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "project": "./tsconfig.json",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {}
}
